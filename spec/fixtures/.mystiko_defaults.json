{
  "defaults": {
    "secretNamePrefix": "/App/HelloWorld/",
    "ignoreSecretNamePrefix": [
      "/Shared/"
    ],
    "secrets": [
      {
        "name": "DEFAULT_ADD_PREFIX",
        "target": "env",
        "envname": "DEFAULT_SECRET_ENV"
      },
      {
        "name": "/Shared/DEFAULT_IGNORE_PREFIX",
        "target": "env",
        "envname": "DEFAULT_SECRET_ENV2"
      }
    ]
  },
  "environments": {
    "local": {
      "region": "us-west-2",
      "secrets": [
        {
          "name": "local_cert.key",
          "target": "file",
          "filename": "./certs/local_cert.key"
        },
        {
          "name": "local_cert.crt",
          "target": "file",
          "filename": "./certs/local_cert.crt"
        },
        {
          "name": "/Shared/essp",
          "target": "env",
          "envname": "ESSP_ENV_VARIABLE"
        }
      ]
    }
  }
}